{
  "compilerOptions": {
    "target": "ESNext",
    "module": "ESNext",
    "moduleResolution": "Bundler",
    "strict": true,
    "skipLibCheck": true,
    "lib": [
      "ESNext",
      "DOM",
      "DOM.Iterable"
    ],
    "types": [
      "@cloudflare/workers-types/2023-07-01",
      "hono"
    ],
    "jsx": "react-jsx",
    "esModuleInterop": true
  },
  "baseUrl": "./", 
  "paths": {
    "@/*": ["./src/*"]
  }
}